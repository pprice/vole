func main() {
    let x: i32? = 42

    // Test that is-check works correctly
    if x is i32 {
        println("x is i32")
    }
    if x is nil {
        println("x is nil")
    }

    // Test with nil value
    let y: i32? = nil
    if y is nil {
        println("y is nil")
    }
    if y is i32 {
        println("y is i32")
    }
}
