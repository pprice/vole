func main() {
    let x = 2
    let result = match x {
        1 => 100
        2 => 200
        _ => 999
    }
    println(result)

    let score = 85
    let grade = match score {
        _ if score >= 90 => 1
        _ if score >= 80 => 2
        _ => 3
    }
    println(grade)

    let n = 7
    println(match n {
        0 => 0
        x => x * x
    })
}
