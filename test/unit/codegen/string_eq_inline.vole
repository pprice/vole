tests "inline string eq intrinsic" {
    test "equal strings by value" {
        let a = "hello"
        let b = "he" + "llo"
        assert(a == b)
    }

    test "different strings are not equal" {
        assert("hello" != "world")
    }

    test "unicode equality" {
        let a = "aðŸ˜€b"
        let b = "aðŸ˜€" + "b"
        assert(a == b)
    }
}
