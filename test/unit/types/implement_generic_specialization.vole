interface Describable {
    func describe() -> string
}

class Tagged<T> {
    value: T
}

tests {
    extend Tagged<i64> with Describable {
        func describe() -> string {
            return "tagged"
        }
    }

    test "implement on concrete specialization" {
        let t = Tagged { value: 42 }
        assert(t.describe() == "tagged")
    }
}
