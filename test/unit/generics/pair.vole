// Test: Generic function with two type parameters
func first<A, B>(a: A, b: B) -> A {
    return a
}

func second<A, B>(a: A, b: B) -> B {
    return b
}

tests {
    test "first returns first argument" {
        assert(first(1, 2) == 1)
        assert(first(true, 42) == true)
    }

    test "second returns second argument" {
        assert(second(1, 2) == 2)
        assert(second(42, false) == false)
    }

    test "mixed type pairs" {
        let x = first(100, 3.14)
        let y = second(100, 3.14)
        assert(x == 100)
        assert(y > 3.0)
    }
}
