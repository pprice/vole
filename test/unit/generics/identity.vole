// Test: Basic generic identity function
func identity<T>(x: T) -> T {
    return x
}

tests {
    test "identity with i64" {
        let x = identity(42)
        assert(x == 42)
    }

    test "identity with bool" {
        let t = identity(true)
        let f = identity(false)
        assert(t == true)
        assert(f == false)
    }

    test "identity with f64" {
        let pi = identity(3.14)
        assert(pi > 3.0)
        assert(pi < 4.0)
    }
}
